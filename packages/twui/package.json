{
  "name": "twui",
  "version": "1.0.0",
  "description": "",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "files": [
    "dist",
    "plugin"
  ],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "rm -rf dist && yarn tw:colorGen && yarn build:esm && yarn build:cjs && yarn tw:build",
    "build:esm": "tsc --outDir ./dist/esm",
    "build:cjs": "tsc --module CommonJS --outDir ./dist/cjs",
    "dev:esm": "tsc --outDir ./dist/esm --watch",
    "dev:cjs": "tsc --module CommonJS --outDir ./dist/cjs --watch",
    "tw:build": "npx tailwindcss -i ./src/styles.css -o ./dist/tkit.css --minify",
    "tw:watch": "npx tailwindcss -i ./src/styles.css -o ./dist/tkit.css --watch",
    "tw:colorGen": "node ./.scripts/safelistColors.js",
    "dev": "yarn concurrently \"yarn tw:watch\" \"yarn storybook\" \"yarn dev:esm\" \"yarn dev:cjs\"",
    "storybook": "start-storybook -p 6006 --no-open",
    "build-storybook": "build-storybook"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
