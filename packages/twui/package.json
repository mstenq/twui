    "glob": "^8.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwindcss": "^3.1.8",
    "tsc": "^2.0.4",
    "typescript": "^4.7.4"
  },
  "peerDependencies": {
